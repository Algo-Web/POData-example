{
    "name": "algo-web/POData-example",
    "description": "Example of using OData on pure PHP with MySQL",
    "keywords": ["odata", "podata", "rest","MySql"],
    "homepage": "https://github.com/Algo-Web",
    "type": "example",
    "license": "LGPL",
    "authors": [
        {
            "name": "Christopher Harris",
            "email": "c.harriss@communicationinnovations.com.au",
            "role": "Creator"
        }
    ],
    "support": {
        "issues": "https://github.com/Algo-Web/POData-example/issues",
        "source": "https://github.com/Algo-Web/POData-example"
    },
    "require": {
        "php": ">=5.4.0",
        "algo-web/podata": "dev-master",
        "wikimedia/composer-merge-plugin": "^1.3"
    },
    "extra": {
        "merge-plugin": {
            "require": [
                "tests/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false
        }
    },
    "autoload": {
        "classmap": [
            "models",
            "src",
            "tests/RawTestCase.php"
        ]
    },
    "minimum-stability": "dev"


}
